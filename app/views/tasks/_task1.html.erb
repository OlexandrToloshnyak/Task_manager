<div class="row">
     <% case task.priority?%>
        <% when nil %>
        <div class="todo_item">
                <p class="pr-nil" style="margin-left: 2em;" title=<%= "Deadline:#{task.deadline}" %>> <%= link_to task.name,  complete_project_task_path(@project, task.id), method: :patch %></p>
            </div>
        <% when "low" %>
            <div class="todo_item">
                <p class="pr-low" style="margin-left: 2em;" title=<%= "Deadline:#{task.deadline}" %>><%= link_to task.name,  complete_project_task_path(@project, task.id), method: :patch %></p>
            </div>
        <% when "normal" %>
            <div class="todo_item">
                <p class="pr-normal" style="margin-left: 2em;" title=<%= "Deadline:#{task.deadline}" %>><%= link_to task.name,  complete_project_task_path(@project, task.id), method: :patch %></p>
            </div>
        <% when "important" %>
            <div class="todo_item">
                <p class="pr-important" style="margin-left: 2em;" title=<%= "Deadline:#{task.deadline}" %>><%= link_to task.name,  complete_project_task_path(@project, task.id), method: :patch %></p>
            </div>
        <% when "critical" %>
            <div class="todo_item">
                <p class="pr-critical" style="margin-left: 2em;" title=<%= "#{task.priority}Deadline:#{task.deadline}" %>><%= link_to task.name,  complete_project_task_path(@project, task.id), method: :patch %></p>
            </div>
            <% else %>
            fasfasfasfasfasfasf
        <% end %>
</div>
